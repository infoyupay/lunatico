<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="https://jakarta.ee/xml/ns/persistence"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
    <persistence-unit name="lunatico">

        <!--Enumerations-->
        <class>com.yupay.lunatico.model.BalanceType</class>
        <class>com.yupay.lunatico.model.DoiType</class>
        <class>com.yupay.lunatico.model.ItemType</class>
        <class>com.yupay.lunatico.model.MovementType</class>
        <class>com.yupay.lunatico.model.VirtualWarehouseType</class>
        <class>com.yupay.lunatico.model.Balance</class>
        <class>com.yupay.lunatico.model.BalanceDetail</class>
        <class>com.yupay.lunatico.model.Item</class>
        <class>com.yupay.lunatico.model.User</class>
        <class>com.yupay.lunatico.model.Movement</class>
        <class>com.yupay.lunatico.model.MovementDetail</class>
        <class>com.yupay.lunatico.model.Person</class>
        <class>com.yupay.lunatico.model.TypeFolio</class>
        <class>com.yupay.lunatico.model.Unit</class>
        <class>com.yupay.lunatico.model.Warehouse</class>
        <class>com.yupay.lunatico.model.OvBalance</class>

        <!--Entites-->


        <properties>
            <property name="eclipselink.jdbc.url"
                      value="jdbc:postgresql://localhost:5432/lunatico_db"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user" value="postgres"/>
        </properties>
    </persistence-unit>
</persistence>
